&lt;/span&gt;&lt;/span&gt;&lt;/span&gt;&lt;span class=&#34;line&#34;&gt;&lt;span class=&#34;cl&#34;&gt;&lt;span class=&#34;cp&#34;&gt;&lt;/span&gt;java17 -server -Xms128m -Xmx256m -jar -Duser.timezone&lt;span class=&#34;o&#34;&gt;=&lt;/span&gt;Asia/Shanghai /home/muyiacc/domains/halo2/halo.jar --spring.config.additional-location&lt;span class=&#34;o&#34;&gt;=&lt;/span&gt;/home/muyiacc/domains/halo2/.halo2/application.yaml
